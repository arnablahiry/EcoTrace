{
  "name": "alpic",
  "version": "1.84.3",
  "description": "The command-line interface for Alpic",
  "homepage": "https://alpic.ai",
  "preferGlobal": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/alpic-ai/alpic.git"
  },
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "chatgpt",
    "app",
    "ai",
    "alpic"
  ],
  "author": "Alpic",
  "license": "ISC",
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@oclif/core": "^4.8.0",
    "chalk": "^5.6.2",
    "ci-info": "^4.4.0",
    "posthog-node": "^5.24.4",
    "tar": "^7.4.0"
  },
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^25.1.0",
    "biome": "^0.3.3",
    "jsdom": "^27.4.0",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "bin": {
    "alpic": "bin/run.js"
  },
  "oclif": {
    "bin": "alpic",
    "commands": "./dist/commands",
    "dirname": "alpic",
    "topicSeparator": " ",
    "hooks": {
      "finally": "./dist/lib/telemetry.js"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "format": "biome check --write --error-on-warnings",
    "test": "pnpm run test:unit && pnpm run test:type && pnpm run test:format",
    "test:unit": "vitest run",
    "test:format": "prettier --check .",
    "test:type": "tsc --noEmit"
  }
}